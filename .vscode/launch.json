{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Deno",
          "type": "node",
          "request": "launch",
          "cwd": "${workspaceFolder}",
          "runtimeExecutable": "deno",
          "attachSimplePort": 9229,
          "program": "${workspaceFolder}/src/Index.ts", 
          "runtimeArgs": ["run", "--inspect-brk", "--allow-all", "--unstable"],
          "outputCapture": "std",
      }
  ]
}
